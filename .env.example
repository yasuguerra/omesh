# Omesh Dashboard Environment Variables
# Copy this file to .env and fill in your actual values.

# Database (MariaDB)
DB_HOST=your_db_host
DB_PORT=3306
DB_USER=your_db_user
DB_PASSWD=your_db_password
DB_DATABASE=your_db_name

# Google Analytics 4
GA_PROPERTY_ID=your_ga4_property_id
# Path to the GA4 service account JSON key file
GA_KEY_PATH=/path/to/your/ga_service_account_key.json

# OpenAI API Key (for AI insights)
OPENAI_API_KEY=your_openai_api_key

# Facebook/Instagram Graph API
FB_ACCESS_TOKEN=your_facebook_page_access_token
# These are usually public IDs but can be stored here if preferred.
# Ensure these correspond to the Facebook Page and Instagram Business Account linked to the FB_ACCESS_TOKEN.
FACEBOOK_ID=your_facebook_page_id_numeric
INSTAGRAM_ID=your_instagram_business_account_id_numeric
